    "class Solution(object):\n",
    "    def climbStairs(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        a = b = 1\n",
    "        n = n-1\n",
    "        for i in range(n):\n",
    "            c = a\n",
    "            a = a+b\n",
    "            b = c\n",
    "        return a"
   ]
