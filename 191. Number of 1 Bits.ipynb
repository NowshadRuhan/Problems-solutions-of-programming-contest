    "class Solution(object):\n",
    "    def hammingWeight(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        count = 0\n",
    "        while n != 0:\n",
    "            if(n&1):\n",
    "                count +=1\n",
    "            n = n >> 1\n",
    "            \n",
    "        return count"
   ]
